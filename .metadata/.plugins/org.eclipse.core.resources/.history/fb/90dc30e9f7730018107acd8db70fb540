<?xml version="1.0" encoding="UTF-8"?>
<proxy name="EchoProxy" startOnLoad="true" statistics="enable" trace="enable" transports="http https" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <log category="DEBUG"/>
            <property name="SERVER_IP" scope="default" type="STRING" value="value"/>
            <respond description=""/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
</proxy>
